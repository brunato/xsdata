{{ obj.name }} [
    label = "{ {{- obj.name }}|

            {%- for attr in obj.attrs -%}
                +{{ attr.name }} : {{ attr.types|join(", ", attribute="name") }}{{ "[]" if attr.is_list else "" }}\l
            {%- endfor -%}
    }"
]

{%- for inner in obj.inner %}
{% with obj=inner -%}
    {%- include "class.jinja2" %}
{%- endwith -%}
{%- endfor %}

{%- for ext in obj.extensions %}
{{ obj.name }} -> {{ ext.name }}[arrowhead=empty]
{%- endfor %}
{%- for inner in obj.inner %}
{{ obj.name }} -> {{ inner.name }}[arrowhead=diamond]
{%- endfor %}
